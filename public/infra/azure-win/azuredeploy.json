{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "siteName": {
            "type": "string",
            "metadata": {
                "description": "Name of azure web app"
            }
        },
        "appServicePlanName": {
            "type": "string",
            "metadata": {
                "description": "Name of hosting plan"
            }
        },
        "skuName": {
            "type": "string",
            "allowedValues": [
                "F1",
                "D1",
                "B1",
                "B2",
                "B3",
                "S1",
                "S2",
                "S3",
                "P1",
                "P2",
                "P3"
            ],
            "defaultValue": "F1",
            "metadata": {
                "description": "SKU Name"
            }
        },
        "capacity": {
            "type": "int",
            "defaultValue": 1,
            "metadata": {
                "description": "Current number of instances assigned to the resource."
            }
        },
        "_artifactsLocation": {
            "type": "string",
            "metadata": {
                "description": "The base URI where artifacts required by this template are located."
            }
        },
        "_artifactsLocationSasToken": {
            "type": "securestring",
            "defaultValue": "",
            "metadata": {
                "description": "The sasToken required to access _artifactsLocation."
            }
        },
        "tags": {
            "type": "object",
            "defaultValue": {
            }
        },
        "location": {
            "type": "string",
            "defaultValue": "[resourceGroup().location]",
            "metadata": {
                "description": "The region to deploy the resources into"
            }
        }
    },
    "functions": [
    ],
    "variables": {
    },
    "resources": [
        {
            "name": "appserviceNestedDeployment",
            "type": "Microsoft.Resources/deployments",
            "apiVersion": "2019-10-01",
            "properties": {
                "mode": "Incremental",
                "templateLink": {
                    "uri": "[concat(parameters('_artifactsLocation'), '/nestedTemplates/appservice.json', parameters('_artifactsLocationSasToken'))]",
                    "contentVersion": "1.0.0.0"
                },
                "parameters": {
                    "siteName": {
                        "value": "[parameters('siteName')]"
                    },
                    "appServicePlanName": {
                        "value": "[parameters('appServicePlanName')]"
                    },
                    "skuName": {
                        "value": "[parameters('skuName')]"
                    },
                    "capacity": {
                        "value": "[parameters('capacity')]"
                    },
                    "tags": {
                        "value": "[parameters('tags')]"
                    }
                }
            }
        },
        {
            "name": "mysqlNestedDeployment",
            "type": "Microsoft.Resources/deployments",
            "apiVersion": "2019-10-01",
            "properties": {
                "mode": "Incremental",
                "templateLink": {
                    "uri": "[concat(parameters('_artifactsLocation'), '/nestedTemplates/mysql.json', parameters('_artifactsLocationSasToken'))]",
                    "contentVersion": "1.0.0.0"
                },
                "parameters": {
                }
            }
        }

    ],
    "outputs": {
    }
}